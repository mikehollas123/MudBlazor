<MudDialog>
    <DialogContent>
        <MudTabs Elevation="4" Rounded="true" Color="@Color.Secondary">
            <MudTabPanel Text="OneOneOneOne">SOME OneOneOneOne</MudTabPanel>
            <MudTabPanel Text="TwoTwoTwoTwo">SOME TwoTwoTwoTwo</MudTabPanel>
            <MudTabPanel Text="ThreeThreeThreeThree">SOME ThreeThreeThreeThree</MudTabPanel>
            <MudTabPanel Text="FourFour">SOME FourFour</MudTabPanel>
            <MudTabPanel Text="FiveFive">SOME FiveFive</MudTabPanel>
            <MudTabPanel Text="SixSixSixSix">SOME SixSixSixSix</MudTabPanel>
        </MudTabs>

    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>


@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}